	<link type="text/css" rel="stylesheet" href="/assets/styles/github-flavored-markdown-pilcrow-hljs.min.css">
</head>
<body>

<article class="markdown-body">

	<h1><%= node.title %></h1>

	<% nodes.map(item => {
		if (item.isDirectory) {

		} else if (item.isFile && item.isMarkdownFile) {

		} else {
			return;
		}
		const link = {
			name: item.title,
			href: path.join('/', item.isFile ? path.dirname(item.toFilePath) : item.toFilePath) + '/',
		};
	%>
	<p>
		<a href="<%= link.href %>"><%= link.name %></a>
	</p>
	<% }); %>

</article>

</body>
</html>
